<StructureDefinition xmlns="http://hl7.org/fhir">
    <id value="program-info"/>
    <url value="http://fhir.org/guides/astp/bhp/StructureDefinition/program-info"/>
    <version value="1.0.0"/>
    <name value="GrantProgramIdAndNumber"/>
    <title value="Grant Program ID and Number"/>
    <fhirVersion value="4.0.1"/>
    <kind value="complex-type"/>
    <abstract value="false"/>
    <context>
        <type value="element"/>
        <expression value="Observation"/>
    </context>
    <type value="Extension"/>
    <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Extension"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Extension">
            <path value="Extension"/>
            <short value="Grant Program ID and Number"/>
        </element>
        <element id="Extension.extension">
            <path value="Extension.extension"/>
            <min value="1"/>
        </element>
        <element id="Extension.extension:program-id">
            <path value="Extension.extension"/>
            <sliceName value="program-id"/>
           <!-- <label value="program_id"/>-->
            <short value="The program id for a program that that is participating in a designated behavioral health program."/>
            <definition value="The program id for a program that that is participating in a designated behavioral health program."/>
            <min value="0"/>
            <max value="1"/>
            <mustSupport value="true"/>
        </element>
        <element id="Extension.extension:program-id.extension">
            <path value="Extension.extension.extension"/>
            <max value="0"/>
        </element>
        <element id="Extension.extension:program-id.url">
            <path value="Extension.extension.url"/>
            <fixedUri value="program-id"/>
        </element>
        <element id="Extension.extension:program-id.value[x]">
            <path value="Extension.extension.value[x]"/>
            <min value="1"/>
            <type>
                <code value="string"/>
            </type>
        </element>
        <element id="Extension.extension:grant-number">
            <path value="Extension.extension"/>
            <sliceName value="grant-number"/>
          <!--  <label value="grant_number"/>-->
            <short value="The grant number that the behavioral health service is operating under."/>
            <definition value="The grant number that the behavioral health service is operating under."/>
            <min value="0"/>
            <max value="1"/>
            <mustSupport value="true"/>
        </element>
        <element id="Extension.extension:grant-number.extension">
            <path value="Extension.extension.extension"/>
            <max value="0"/>
        </element>
        <element id="Extension.extension:grant-number.url">
            <path value="Extension.extension.url"/>
            <fixedUri value="grant-number"/>
        </element>
        <element id="Extension.extension:grant-number.value[x]">
            <path value="Extension.extension.value[x]"/>
            <type>
                <code value="string"/>
            </type>
        </element>
        <element id="Extension.url">
            <path value="Extension.url"/>
            <fixedUri value="http://fhir.org/guides/astp/bhp/StructureDefinition/program-info"/>
        </element>
        <element id="Extension.value[x]">
            <path value="Extension.value[x]"/>
            <max value="0"/>
        </element>
    </differential>
</StructureDefinition>